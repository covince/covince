{
  "area_search_terms": {
    "url": "./data/area-search.json",
    "mode": "padded-first-input-token"
  },
  "dynamic_mode": {
    "initial_lineages": {
      "B": 2,
      "B.1.617.2": 1,
      "AY.4.2": 0
    },
    "_colour_palette": [
      "magenta",
      "#0F0",
      "cyan"
    ]
  },
  "parameters": [
    {
      "id": "lambda",
      "display": "Incidence",
      "_precision": {
        "mean": 2,
        "range": 1
      }
    },
    {
      "id": "p",
      "display": "Proportion",
      "format": "percentage",
      "precision": 2
    },
    {
      "id": "R",
      "display": "R"
    }
  ],
  "ontology": {
    "overview": {
      "category": "National Overview",
      "heading": "England",
      "short_heading": "National"
    },
    "area": {
      "category": "Local authority",
      "noun_plural": "local authorities",
      "search_placeholder": "local authority or postcode"
    }
  },
  "map": {
    "settings": {
      "default_color_scale": {
        "p": "linear",
        "lambda": "quadratic"
      },
      "color_map_domain": {
        "p": {
          "min": 0,
          "max": 1
        },
        "lambda": {
          "min": 0
        },
        "R": {
          "min": 0,
          "max": 4
        }
      }
    },
    "viewport": {
      "min_zoom": 4,
      "default_lat": 52.561928,
      "default_lon": -1.464854,
      "default_zoom": {
        "mobile": 4.5,
        "desktop": 5,
        "big": 5.5
      },
      "bounds": {
        "min_longitude": -9,
        "max_longitude": 5,
        "min_latitude": 48,
        "max_latitude": 60
      }
    }
  },
  "charts": [
    {
      "parameter": "lambda",
      "heading": "Incidence",
      "default_type": "line",
      "allow_stack": true
    },
    {
      "parameter": "p",
      "heading": "Proportion",
      "default_type": "area",
      "allow_stack": true
    },
    {
      "parameter": "R",
      "heading": "R",
      "default_type": "line",
      "y_axis": {
        "ticks": [
          "0",
          "1",
          "2",
          "3"
        ],
        "domain": [
          0,
          3
        ],
        "allow_data_overflow": true,
        "reference_line": 1
      }
    }
  ],
  "nomenclature": [
    {
      "lineage": "B.1.1.7",
      "alt_name": "Alpha"
    }
  ]
}
